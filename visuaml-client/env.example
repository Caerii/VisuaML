# VisuaML Environment Configuration

# ==========================================
# Clerk Authentication (Optional)
# ==========================================
# Get your publishable key from https://dashboard.clerk.com/
# If not provided, app runs without authentication
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here

# ==========================================
# Python Configuration (Recommended)
# ==========================================
# Path to Python executable with required packages
# If not set, defaults to 'python' command
# 
# Examples by platform:
# Windows Conda: VISUAML_PYTHON=C:\Users\YourUser\miniconda3\envs\visuaml\python.exe
# Windows venv:  VISUAML_PYTHON=visuaml-env\Scripts\python.exe
# Linux/Mac Conda: VISUAML_PYTHON=/home/youruser/miniconda3/envs/visuaml/bin/python
# Linux/Mac venv: VISUAML_PYTHON=visuaml-env/bin/python
# System default: VISUAML_PYTHON=python
VISUAML_PYTHON=python

# ==========================================
# WebSocket Configuration (Optional)
# ==========================================
# URL for real-time collaboration server
VITE_WEBSOCKET_URL=ws://localhost:1234

# ==========================================
# API Configuration (Optional)
# ==========================================
# Base URL for the VisuaML API server
VITE_API_BASE_URL=http://localhost:8787

# ==========================================
# Development Configuration (Optional)
# ==========================================
# Enable debug mode for development
VITE_DEBUG_MODE=false

# Set log level (error, warn, info, debug)
VITE_LOG_LEVEL=info

# OpenMP configuration (usually handled automatically)
# Set to TRUE if you encounter OpenMP conflicts
KMP_DUPLICATE_LIB_OK=TRUE 